llAll: [],\n      zhu: 0,\n      money: 0,\n      bei: 1,\n      resultObj: {}\n    }\n  },\n  created () {\n    console.log(this.$store.state.dlt)\n    this.selectedBallAll = this.$store.state.dlt\n    this.countMoney()\n  },\n  methods: {\n    back: function () {\n      this.$router.push(\n        '/Dlt'\n      )\n    },\n    randBall: function () {\n      this.redBallArr = randRedBall()\n      this.blueBallArr = randBlueBall()\n      this.selectedBallAll.unshift(countBall(this.redBallArr, this.blueBallArr))\n      this.countMoney()\n      store.commit('setDlt', this.selectedBallAll)\n    },\n    countMoney: function () {\n      this.zhu = calZhu(this.selectedBallAll)\n      this.money = this.zhu * 2 * this.bei\n      let resultObj = {}\n      resultObj.issue = '20191111'\n      resultObj.bei = 1\n      resultObj.zhuijia = 0\n      resultObj.caipiao_type = 'dlt'\n      resultObj.sum_money = this.money\n      resultObj.touzhuInfo = this.selectedBallAll\n      resultObj.quan_id = 0\n      resultObj.username = 'user888'\n      this.resultObj = resultObj\n      // console.log('resultObj:', this.resultObj)\n    },\n    delBall: function (index) {\n      this.selectedBallAll.splice(index, 1)\n      this.countMoney()\n    },\n    touzhu: function () {\n      postData('http://lottery1.localhost/Touzhu/touzhu', this.resultObj)\n        .then(res => {\n          console.log(res)\n          if (res.code === 1) {\n            alert('投注成功')\n          }\n        })\n    }\n  },\n  computed: {\n    codeList () {\n      console.log('--', this.$store.state.dlt)\n      return this.$store.state.dlt\n    }\n  }\n}\n</script>\n\n<style lang='stylus' scoped>\n@import '../common/css/public.styl'\n.content\n  position fixed;\n  left 0rem;\n  top 0rem\n  bottom 0rem;\n  right 0rem;\n  z-index 101;\n  background #fff;\n  margin 0;\n  padding 0;\n  border-radius 0;\n  .header\n    width 10rem;\n    height 1rem;\n    margin 0 auto;\n    position fixed;\n    left 0rem;\n    top 0.5rem;\n    